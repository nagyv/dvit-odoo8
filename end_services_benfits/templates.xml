<openerp>
    <data>
        <record id="hr_payslip_view_endservicesbenefits" model="ir.ui.view">
            <field name="name">hr.payslip.view.form.endservicesbenefits</field>
            <field name="model">hr.payslip</field>
            <field name="inherit_id" ref="hr_payroll.view_hr_payslip_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@string='Accounting Information']" position="after">
                <page string="End services benefits">
                    <group col="4">
                    <field name="employee_id"/>
                    <field name="from_date"/>
                    <field name="to_date"/>
                    <field name="work_years"/>
                    </group>
                </page>
                </xpath>
            </field>
        </record>
    </data>
</openerp>